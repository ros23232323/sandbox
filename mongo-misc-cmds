mongo
help
show dbs
db
use parse
show collections
db.Race.find().limit(5)

mongo 
use parse
db.Race.find({_id:"l8Z3zmd60W"}).pretty()
exit


cat output.json | jq '.'



mongo localhost:27017/parse --eval "db.Race.find({_id:\"l8Z3zmd60W\"}).pretty()" >> sample.json

#get first 5 races from all meetings in Galway
mongo localhost:27017/local --quiet --eval "db.getCollection('Meeting').find({'track':'Galway'},{'races':1}).pretty()" | jq '.races[].objectId' | tr -d "\"" | head -n 5


#Export collection
mongoexport --db dev --collection Topic --out Topic.json

#Import collection
mongoimport --db local --collection Topic --file Topic.json


#Export database
mongodump -d dev -o /tmp
cd /tmp
tar -cvzf dev.tar.bzip2 dev

#Import database
mongorestore -d local dev/


#query by id
db.Address.find({"_id":"bPNJsnBbr6"})

#returns all docs to limit
db.Address.find()


