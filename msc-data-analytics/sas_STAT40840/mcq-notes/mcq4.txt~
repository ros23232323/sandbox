proc univariate data=work.bodyweight1;
var bodyweight0;
output out=work.means mean=m_weight0 ;
run;


proc univariate data=work.test cibasic;
by block;
var nitrate ammonia;
run;
		
proc univariate data=work.test cibasic;
var block nitrate ammonia;
run;
		
proc univariate data=work.test cibasic;
var nitrate ammonia;
run;
		
proc univariate data=work.test cibasic noprint;
by block;
var nitrate ammonia;
run;



proc univariate data=work.test noprint;
class block;
histogram response;
run;
		
proc univariate data=work.test noprint;
by block;
histogram response;
run;
		
proc univariate data=work.test noprint;
histogram response / group=block;
run;
