#list topics
$KAFKA_HOME/bin/kafka-topics.sh --list --zookeeper localhost:2181

$KAFKA_HOME/bin/kafka-topics.sh --describe --zookeeper localhost:2181
$KAFKA_HOME/bin/kafka-topics.sh --describe --topic test --zookeeper localhost:2181

$KAFKA_HOME/bin/kafka-console-producer.sh --broker-list localhost:9092 --topic test


$KAFKA_HOME/bin/kafka-consumer-offset-checker.sh --group --topic test --zookeeper localhost:2181
